#include <stdio.h>
#include <math.h>
int main() {
    float T, V;
    float g;
    g = 10;
    printf("Введите значение для T ");
    scanf("%f", &T);
    printf("Введите значение для V ");
    scanf("%f", &V);
    float a;
    a =((asin((g*T)/(2*V)))*(180/3.14));
    printf("a = %f", a); 
    return 0;
}
