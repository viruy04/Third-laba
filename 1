#include <stdio.h>
int main() {
    float a,b;
    printf("Введите значение для а ");
    scanf("%f", &a);
    printf("Введите значение для b ");
    scanf("%f", &b);
    if(a>b){
        printf("Больше");
    } else if (a<b){
        printf("Меньше");
    } else if (a=b){
        printf("Эти числа равны");
    }
    return 0;
}
